        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^4.0",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/",
            "Tests\\": "tests/"
        }
    }
}
